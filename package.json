{
  "name": "journey_home",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:headed": "playwright test --headed",
    "test:debug": "playwright test --debug",
    "test:report": "playwright show-report",
    "firebase:emulator": "firebase emulators:start",
    "firebase:emulator:ui": "firebase emulators:start --only firestore,storage",
    "firebase:emulator:export": "firebase emulators:export ./firebase-export",
    "firebase:emulator:import": "firebase emulators:start --import ./firebase-export",
    "firebase:deploy": "firebase deploy",
    "firebase:deploy:hosting": "firebase deploy --only hosting",
    "firebase:deploy:firestore": "firebase deploy --only firestore",
    "firebase:deploy:storage": "firebase deploy --only storage",
    "firebase:deploy:functions": "firebase deploy --only functions",
    "functions:install": "cd functions && npm install",
    "functions:build": "cd functions && npm run build",
    "functions:test": "cd functions && npm test",
    "functions:test:coverage": "cd functions && npm run test:coverage",
    "functions:serve": "cd functions && npm run serve",
    "test:all": "npm run test && npm run functions:test"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "firebase": "^10.13.0",
    "lucide-react": "^0.548.0",
    "next": "15.5.3",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.55.0",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.0",
    "eslint": "^9",
    "eslint-config-next": "15.5.3",
    "firebase-tools": "^13.0.0",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
